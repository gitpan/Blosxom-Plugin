package not_found;
use strict;
use warnings;
use base qw/Blosxom::Plugin/;

__PACKAGE__->load_plugin( 'DataSection' );

sub start { !$blosxom::static_entries }

sub last {
    my $class = shift;
    #$blosxom::output = $class->data_section->{'404.html'};
    $blosxom::output = $class->data_section->get( '404.html' );
}

1;

